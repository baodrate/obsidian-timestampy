{
	"name": "obsidian-timestampy",
	"version": "0.1.0",
	"description": "An obsidian plugin to track last modified and created dates of notes",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"lint": "biome check",
		"format": "biome format --write",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "@baodrate",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^16.11.6",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.24.0",
		"obsidian": "^1.7.2",
		"obsidian-typings": "^2.3.3",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
