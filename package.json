{
	"name": "obsidian-timestampy",
	"version": "0.1.0",
	"description": "An obsidian plugin to track last modified and created dates of notes",
	"main": "main.js",
	"scripts": {
		"prebuild": "pnpm run lint:typescript",
		"build": "vite build",
		"dev": "vite build --watch --mode=development",
		"format": "biome format --write",
		"lint": "pnpm run /^lint:.*/",
		"lint:biome": "biome check",
		"lint:typescript": "tsc -noEmit -skipLibCheck",
		"preinstall": "npx only-allow pnpm",
		"prepare": "husky",
		"version": "node version-bump.mjs"
	},
	"keywords": [],
	"author": "@baodrate",
	"url": "https://github.com/baodrate",
	"repository": "github:baodrate/obsidian-timestampy",
	"homepage": "https://github.com/baodrate/obsidian-timestampy#readme",
	"bugs": "https://github.com/baodrate/obsidian-timestampy/issues",
	"funding": "https://github.com/sponsors/baodrate",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@commitlint/types": "^19.8.1",
		"@types/node": "^24.0.7",
		"builtin-modules": "5.0.0",
		"husky": "^9.1.7",
		"i18next": "^25.3.0",
		"obsidian": "^1.8.7",
		"obsidian-typings": "^3.12.0",
		"tslib": "2.8.1",
		"typescript": "5.8.3",
		"vite": "npm:rolldown-vite@^7.0.1",
		"vite-plugin-static-copy": "^3.1.0"
	},
	"packageManager": "pnpm@10.12.3+sha512.467df2c586056165580ad6dfb54ceaad94c5a30f80893ebdec5a44c5aa73c205ae4a5bb9d5ed6bb84ea7c249ece786642bbb49d06a307df218d03da41c317417"
