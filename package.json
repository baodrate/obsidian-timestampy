{
	"name": "obsidian-timestampy",
	"version": "0.1.3",
	"description": "An obsidian plugin to track last modified and created dates of notes",
	"main": "main.js",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"lint": "biome check",
		"format": "biome format --write",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "@baodrate",
	"url": "https://github.com/baodrate",
	"repository": "github:baodrate/obsidian-timestampy",
	"homepage": "https://github.com/baodrate/obsidian-timestampy#readme",
	"bugs": "https://github.com/baodrate/obsidian-timestampy/issues",
	"funding": "https://github.com/sponsors/baodrate",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.0.0-beta.1",
		"@types/node": "^22.15.12",
		"builtin-modules": "5.0.0",
		"esbuild": "^0.25.4",
		"obsidian": "^1.8.7",
		"obsidian-typings": "^3.7.0",
		"tslib": "2.8.1",
		"typescript": "5.8.3"
	},
	"packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971"
}
