{
	"name": "obsidian-timestampy",
	"version": "0.1.0",
	"description": "An obsidian plugin to track last modified and created dates of notes",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"lint": "biome check",
		"format": "biome format --write",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "@baodrate",
	"url": "https://github.com/baodrate",
	"repository": "github:baodrate/obsidian-timestampy",
	"homepage": "https://github.com/baodrate/obsidian-timestampy#readme",
	"bugs": "https://github.com/baodrate/obsidian-timestampy/issues",
	"funding": "https://github.com/sponsors/baodrate",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.13.4",
		"builtin-modules": "4.0.0",
		"esbuild": "^0.25.0",
		"obsidian": "^1.7.2",
		"obsidian-typings": "^2.23.6",
		"tslib": "2.8.1",
		"typescript": "5.7.3"
	}
}
