	"description": "An obsidian plugin to track last modified and created dates of notes",
	"main": "main.js",
	"scripts": {
		"prebuild": "pnpm run lint:typescript",
		"build": "vite build",
		"dev": "vite build --watch --mode=development",
		"format": "biome format --write",
